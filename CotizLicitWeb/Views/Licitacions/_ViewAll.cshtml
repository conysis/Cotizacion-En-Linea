@model IEnumerable<CotizLicitWeb.Models.Licitacion>

    <p>
        <a asp-action="Create">Crear Nueva Licitación</a>
    </p>
    <table class="table">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Expediente)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.FecCreacion)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.FecApertura)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.IdProveedor)
                </th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Expediente)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.FecCreacion)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.FecApertura)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.IdProveedor)
                    </td>
                    <td>
                        <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                        <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                        <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
                    </td>
                </tr>
            }
        </tbody>
    </table>
